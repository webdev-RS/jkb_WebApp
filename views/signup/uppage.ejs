<link href="signupcss.css" rel="stylesheet">
<div class="main">
<form action="" method="post">
     <div class="item">
    <label>User/Creater Name  </label>
         <input type="text" name="username"  placeholder="Enter Your Name"  value="<%= uname.username %>"required>
     </div>
    <br>
    <div class="item">
    <label>Email</label>
         <input type="text" name="email" placeholder="Enter Your Email" value="<%= uname.email %>" required>
     </div>
    <br>
    <div class="item">
    <label>Password</label>
     <input type="password" name="pass" placeholder="Enter Password" value="<%= uname.pass %>"required>
     </div>
    <br>
    <div class="form-btn">
      <button class="btn1" type="submit">Login</button>
      <button class="btn2" role="button">
           <a href="/">Cancel</a>
      </button>
    </div>
</form>
</div>

